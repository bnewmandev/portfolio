<div class="form-group">
    <label for="id">ID</label>
    <input required value="<%= project.id %>" type="text" name="id" id="id" class="form-control">
</div>

<div class="form-group">
    <label for="img">Image Link</label>
    <input required value="<%= project.img %>" type="text" name="img" id="img" class="form-control">
</div>

<div class="form-group">
    <label for="title">Title</label>
    <input required value="<%= project.title %>" type="text" name="title" id="title" class="form-control">
</div>

<div class="form-group">
    <label for="link">Github Link</label>
    <input required value="<%= project.link %>" type="text" name="link" id="link" class="form-control">
</div>

<div class="form-group">
    <label for="text">Text</label>
    <textarea required rows="25" style="height:100%;" type="text" name="text" id="text"
              class="form-control"><%= project.text %></textarea>
</div>

<a href="/articles" class="btn btn-secondary">Cancel</a>
<button type="submit" class="btn btn-primary">Save</button>